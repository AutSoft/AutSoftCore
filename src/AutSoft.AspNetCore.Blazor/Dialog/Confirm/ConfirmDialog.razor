@inherits DialogComponentBase<ConfirmDialogParameters, bool>

<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h5">@Parameter!.Title</MudText>
    </TitleContent>
    <DialogContent>
        <MudText Typo="Typo.body1">@Parameter!.Description</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Text" OnClick="@((e) => Close(false))">Cancel</MudButton>
        <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick="@((e) => Close(true))">Yes</MudButton>
    </DialogActions>
</MudDialog>
